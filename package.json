{
  "name": "@unify-js/vue-webpack-tool",
  "version": "0.3.0",
  "description": "A webpack-based build tool for Vue development that focuses on the build performance.",
  "type": "module",
  "main": "./bin/cli.js",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc && chmod +x ./bin/cli.js",
    "format": "prettier ./src --write",
    "cz": "cz",
    "cs": "changeset",
    "csv": "changeset version",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "bin": {
    "vue-webpack-tool": "./bin/cli.js"
  },
  "files": [
    "bin"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.10.4",
    "@types/webpack": "^5.28.5",
    "cz-conventional-changelog": "^3.3.0",
    "prettier": "3.1.1",
    "typescript": "^5.3.3",
    "vitepress": "^1.0.0-rc.31"
  },
  "peerDependencies": {
    "less": "4.x",
    "sass": "1.x",
    "vue": "3.x"
  },
  "peerDependenciesMeta": {
    "less": {
      "optional": true
    },
    "sass": {
      "optional": true
    }
  },
  "dependencies": {
    "commander": "^11.1.0",
    "css-loader": "^6.8.1",
    "esbuild-loader": "^4.0.2",
    "html-webpack-plugin": "^5.5.4",
    "less-loader": "^11.1.3",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "vue-loader": "^17.3.1",
    "vue-template-compiler": "^2.7.15",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
